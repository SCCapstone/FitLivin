<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/containerFitnessP"
    android:background="@drawable/background2"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linview"
        android:layout_gravity="center_horizontal|bottom">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MAX"
            android:id="@+id/textView2"
            android:layout_gravity="center_horizontal|top"
            android:textColor="#fffef7"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current Bench (lbs):"
        android:id="@+id/benchText"
        android:layout_gravity="left|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/textView2"


         />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current Squat (lbs):"
        android:id="@+id/squatText"
        android:layout_gravity="left|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/benchText"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current DeadLift (lbs):"
        android:id="@+id/deadliftText"
        android:layout_gravity="left|top"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/squatText"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MileTime (sec):"
        android:id="@+id/mileText"
        android:layout_gravity="left|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/totalText"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BACK"
        android:id="@+id/MaxBack"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Big 3 Total:"
        android:id="@+id/totalText"
        android:textColor="#ffffff"
        android:textSize="35dp"
        android:layout_below="@+id/deadliftText"
        android:layout_alignStart="@+id/MaxBack"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/userTotal"
        android:textColor="#ffffff"
        android:textSize="35dp"
        android:text="0"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/totalText"
        android:layout_below="@+id/deadliftText"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/userDeadlift"
        android:layout_gravity="right|center_vertical"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:text="0"
        android:layout_toEndOf="@+id/deadliftText"
        android:layout_below="@+id/squatText"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/userSquat"
        android:layout_gravity="right|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:text="0"
        android:layout_toEndOf="@+id/squatText"
        android:layout_below="@+id/benchText"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/userBench"
        android:layout_gravity="right|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:textIsSelectable="true"
        android:text="0"
        android:layout_toEndOf="@+id/benchText"
        android:layout_below="@+id/textView2"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/userTime"
            android:layout_gravity="right|top"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textIsSelectable="true"
            android:text="0"
            android:layout_toEndOf="@+id/mileText"
            android:layout_below="@+id/totalText"/>

</RelativeLayout>
</FrameLayout>
