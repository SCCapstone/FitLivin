<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/containerFitnessP"
    android:background="@drawable/background2"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linview"
        android:layout_gravity="center_horizontal|bottom">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MAX"
            android:id="@+id/textView2"
            android:layout_gravity="center_horizontal|top"
            android:textColor="#ffffff"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current Bench (lbs):"
        android:id="@+id/benchText"
        android:layout_gravity="left|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/textView2"


         />

        <EditText
            android:layout_width="130dp"
            android:layout_height="37dp"
            android:id="@+id/changeBench"
            android:inputType="number"
            android:layout_gravity="left|center_vertical"
            android:background="#ffffff"
            android:hint="Bench"
            android:layout_below="@+id/benchText"
            android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current Squat (lbs):"
        android:id="@+id/squatText"
        android:layout_gravity="left|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/changeBench"
        />

        <EditText
            android:layout_width="130dp"
            android:layout_height="37dp"
            android:id="@+id/changeSquat"
            android:inputType="number"
            android:layout_gravity="left|center_vertical"
            android:background="#ffffff"
            android:hint="Squat"
            android:layout_below="@+id/squatText"
            android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current DeadLift (lbs):"
        android:id="@+id/deadliftText"
        android:layout_gravity="left|top"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/changeSquat"
        />

        <EditText
            android:layout_width="130dp"
            android:layout_height="37dp"
            android:id="@+id/changeDeadlift"
            android:inputType="number"
            android:layout_gravity="left|center_vertical"
            android:background="#ffffff"
            android:hint="Deadlift"
            android:layout_below="@+id/deadliftText"
            android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MileTime (sec):"
        android:id="@+id/mileText"
        android:layout_gravity="left|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_alignStart="@+id/MaxBack"
        android:layout_below="@+id/changeBig3"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BACK"
        android:id="@+id/MaxBack"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Big 3 Total:"
        android:id="@+id/totalText"
        android:textColor="#ffffff"
        android:textSize="35dp"
        android:layout_below="@+id/changeDeadlift"
        android:layout_alignStart="@+id/MaxBack"/>

        <EditText
            android:layout_width="130dp"
            android:layout_height="37dp"
            android:id="@+id/changeBig3"
            android:inputType="number"
            android:layout_gravity="left|center_vertical"
            android:background="#ffffff"
            android:hint="Total"
            android:layout_below="@+id/totalText"
            android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/userTotal"
        android:textColor="#ffffff"
        android:textSize="35dp"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/totalText"
        android:layout_below="@+id/changeDeadlift"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/userDeadlift"
        android:layout_gravity="right|center_vertical"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:layout_toEndOf="@+id/deadliftText"
        android:layout_below="@+id/changeSquat"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/userSquat"
        android:layout_gravity="right|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:layout_toEndOf="@+id/squatText"
        android:layout_below="@+id/changeBench"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/userBench"
        android:layout_gravity="right|top"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:textIsSelectable="true"
        android:layout_toEndOf="@+id/benchText"
        android:layout_below="@+id/textView2"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/userTime"
            android:layout_gravity="right|top"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:textIsSelectable="true"
            android:layout_toEndOf="@+id/mileText"
            android:layout_below="@+id/changeBig3"/>

        <EditText
            android:layout_width="130dp"
            android:layout_height="37dp"
            android:id="@+id/changeMileTime"
            android:inputType="number"
            android:layout_gravity="left|center_vertical"
            android:background="#ffffff"
            android:hint="MileTime"
            android:layout_below="@+id/userTime"
            android:layout_alignParentStart="true" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enter"
            android:id="@+id/enterMax"
            android:layout_alignTop="@+id/MaxBack"
            android:layout_alignParentEnd="true" />



    </RelativeLayout>
</FrameLayout>
